<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="General" targetNamespace="http://tempuri.org/General.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/General.xsd" xmlns:mstns="http://tempuri.org/General.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="root">
