                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
                      P.ProductName, COALESCE (CONVERT(VARCHAR(MAX), EPS.Description), '') AS Description, C.CompanyShortName, 
                      CASE WHEN P.PRODUCTTYPEID = 2 AND EPS.HTMLFILE IS NULL AND EPS.DESCRIPTION IS NOT NULL AND P.PARENTID IS NULL 
                      THEN COALESCE(PD.HTMLFILE,'') WHEN P.PRODUCTTYPEID = 2 AND EPS.HTMLFILE IS NULL AND EPS.DESCRIPTION IS NOT NULL AND P.PARENTID IS NOT NULL 
                      THEN COALESCE (PD.HTMLFILE, '') ELSE COALESCE (EPS.HTMLFILE, '') END AS HtmlFile
FROM         Products AS P INNER JOIN
                      EditionProductSections AS EPS ON P.PRODUCTID = EPS.PRODUCTID INNER JOIN
                      ProductIndexes AS PIN ON EPS.ProductId = PIN.ProductId INNER JOIN
                      Companies AS C ON P.CompanyId = C.CompanyId INNER JOIN
                      CompanyEditions AS CE ON C.CompanyId = CE.CompanyId And EPS.EditionId = CE.EditionId
INNER JOIN
                      ProductTypes AS PT ON P.ProductTypeId = PT.ProductTypeId LEFT OUTER JOIN
                          (SELECT     ProductId, HtmlFile,EditionId
                            FROM          EditionProductSections) AS PD ON P.ParentId = PD.ProductId And EPS.EditionId = PD.EditionId 
WHERE     (CE.EditionId =@editionId) AND (PIN.IndexId = @indexId) AND (P.ProductName COLLATE SQL_Latin1_General_CP1_CI_AI LIKE @letter + '%')
ORDER BY P.ProductName, C.CompanyShortName</CommandText>
