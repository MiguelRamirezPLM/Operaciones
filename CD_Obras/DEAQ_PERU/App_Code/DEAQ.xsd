              <DbSource ConnectionRef="DEAQ PERUConnectionString (Web.config)" DbObjectName="DEAQ.dbo.plm_vwProductsByEdition" DbObjectType="View" FillMethodModifier="Public" FillMethodName="GetProducts1" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetProducts" GeneratorSourceName="GetProducts1" GetMethodModifier="Public" GetMethodName="GetProducts" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetProducts" UserSourceName="GetProducts1">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT v.DivisionShortName AS Empresa, 
			v.ProductName AS Nombre, 
			COALESCE (v.ProductDescription, '') AS Description, 
			COALESCE (v.Register, '') AS Registro, 
            v.ProductId, v.PharmaFormId, 
            dbo.plm_dfGetPharmaFormsByProduct(v.ProductId, v.EditionId) AS PharmaForm,
