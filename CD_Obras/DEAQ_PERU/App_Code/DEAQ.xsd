				WHEN htmlcontent is null THEN 'Semillas.pdf'
				ELSE convert(varchar(20), v.ProductId) + '_' + convert(varchar(20), v.PharmaFormId) 
			END	AS FileName
FROM  plm_vwProductsByEdition v
LEFT JOIN ParticipantProducts pp on v.EditionId = pp.EditionId
								and v.DivisionId = pp.DivisionId
								and v.CategoryId = pp.CategoryId
								and v.ProductId = pp.ProductId
								and v.PharmaFormId = pp.PharmaFormId
WHERE (v.EditionId = @Edition ) AND (v.ProductName LIKE @letra + '%') AND TypeInEdition = 'P'
ORDER BY Nombre</CommandText>
