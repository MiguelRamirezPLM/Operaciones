                    <CommandText>SELECT DISTINCT 
                         v.DivisionId, v.DivisionShortName AS DivisionName, v.ProductId, v.ProductName, v.Page, dbo.plm_dfGetActiveSubsByProduct(v.ProductId) AS Substance, 
