                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>				select distinct		
									(SELECT EditionId FROM Editions where EditionId = @EditionId) AS EditionId,
									(SELECT NumberEdition FROM Editions where EditionId = @EditionId) AS NumberEdition,
									c.ClientId, 
									c.CompanyName,
									ct.CategoryThreeId,
									ct.CategoryThree,
									a.AdvertId,
									a.AdvertName,
									a.AdvertDescription,
									a.AdvertFile,
									at.Description,
null as Count,
null as Adviser
									
									
				from ClientAdvertCategories cac
				INNER JOIN ClientAdverts ca on cac.AdvertId = ca.AdvertId
										   and cac.ClientId = ca.ClientId
				INNER JOIN CategoryThree ct on cac.CategoryThreeId = ct.CategoryThreeId
				INNER JOIN AdvertTypes at on cac.AdvertTypeId = at.AdvertTypeId
				inner join Adverts a on ca.AdvertId = a.AdvertId
				inner join Clients c on ca.ClientId = c.ClientId
				where	ca.ClientId = @ClientId and
						a.Active = 1 
</CommandText>
