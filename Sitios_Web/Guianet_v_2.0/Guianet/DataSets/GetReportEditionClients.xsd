                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
		SELECT	DISTINCT

				e.EditionId,
				e.NumberEdition,
				
				c.ClientId,
				c.CompanyName,
				
				ct.ClientTypeId,
				ct.TypeName,
				
				ec.Page,

                                                                          null as Count,
                                                                          null as Date,
                                                                          null as UserName

		FROM EditionClients ec
		INNER JOIN Editions e on ec.EditionId = e.EditionId
		INNER JOIN Clients c on ec.ClientId = c.ClientId
		INNER JOIN ClientTypes ct on ec.ClientTypeId = ct.ClientTypeId
		INNER JOIN PLMUsers.DBO.Users U ON @UserId = U.UserId
		WHERE ec.AddedDate &gt;= CONVERT(varchar(10), @Date, 112) AND
		ec.AddedDate &lt; CONVERT(varchar(10), @Date + 1, 112) and ec.ClientTypeId is not null
		order by e.EditionId, c.CompanyName</CommandText>
