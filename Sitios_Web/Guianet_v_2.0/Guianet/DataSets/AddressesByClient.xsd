                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select	DISTINCT 
			  cl.[CompanyName],
                                                          cl.[ShortName]
			  ,a.[Address]
			  ,a.[ZipCode]
			  ,a.[City]
			  ,a.[Email]
			  ,a.[Web]
			  ,a.[Active]
			  ,a.[CountryId]
			  ,c.[CountryName]
			  ,a.[AddressId]
			  ,a.[Suburb]
			  ,a.[StateId]
			  ,s.[StateName]
			  ,a.[Location]
			  ,a.[Longitud]
			  ,a.[Latitud],

                                                         NULL AS Branchs,
NULL AS Brands,
NULL AS Adverts,
NULL AS Products,

NULL AS Phone,
NULL AS Fax,
NULL AS PhoneFax,
NULL AS Lada,
NULL AS Adviser

		from ClientAddresses ca
		inner join Addresses a on ca.AddressId = a.AddressId
		left join States s on a.StateId = s.StateId
		left join Countries c on a.CountryId = c.CountryId
		inner join Clients cl on ca.ClientId = cl.ClientId
		where ca.ClientId = @ClientId
		order by 1</CommandText>
