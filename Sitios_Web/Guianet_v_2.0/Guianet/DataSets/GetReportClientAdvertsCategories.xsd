                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select	DISTINCT
						e.EditionId,
						e.NumberEdition,
						
						c.ClientId,
						c.CompanyName,
						c.ShortName,
						
						a.AdvertId,
