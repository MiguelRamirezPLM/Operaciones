null as Count,
null as UserName,
null as Executive,
null as Date,
null as DateOfReport,
null as Adviser

						
						from ClientAdvertCategories cac
						inner join Editions e on cac.EditionId = e.EditionId
						inner join  Clients c on cac.ClientId = c.ClientId
						inner join  Adverts a on cac.AdvertId = a.AdvertId
						inner join  AdvertTypes at on cac.AdvertTypeId = at.AdvertTypeId
						inner join CategoryThree ct on cac.CategoryThreeId = ct.CategoryThreeId
						where	cac.ClientId = @ClientId AND 
								cac.AdvertId = @AdvertId AND 
								cac.EditionId = @EditionId AND 
								cac.CategoryThreeId = @CategoryThreeId AND 
								cac.AdvertTypeId = @AdvertTypeId AND 
								cac.AddedDate &gt;= CONVERT(varchar(10), @Date, 112) AND
								cac.AddedDate &lt; CONVERT(varchar(10), @Date+1, 112) </CommandText>
