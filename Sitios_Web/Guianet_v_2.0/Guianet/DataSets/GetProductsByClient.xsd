                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select distinct p.ProductId, 
			p.ProductName, 
			
			cp.ClientId,
                                                        null as CompanyName,
			cp.RegisterSanitary,
			
			bc.BarCodeId,
			bc.BarCode,
			
			null as SIDEF,
			
			null as PP,
			
			null as  CC,
			
			null as SC,
		     
		                  null as MP,

                                                      null as EditionId,

                                                     null as Count,

                                                    null as CategoryThree,
                                                    null as LeafCategory,

null as Adviser,
null as CountProduct
			
		from Products p
		inner join ClientProducts cp on p.ProductId = cp.ProductId
		inner join ParticipantProducts pp on @clientId = pp.ClientId
										and @EditionId = pp.EditionId
										and p.ProductId = pp.ProductId
		left join EditionClientProductSIDEF ecps on @clientId = ecps.ClientId
												and @EditionId = ecps.EditionId
												and p.ProductId = ecps.ProductId
		left join ClientProductBarCodes cpbc on cp.ProductId = cpbc.ProductId
		left join BarCodes bc on cpbc.BarCodeId = bc.BarCodeId
		where cp.ClientId = @clientId
</CommandText>
