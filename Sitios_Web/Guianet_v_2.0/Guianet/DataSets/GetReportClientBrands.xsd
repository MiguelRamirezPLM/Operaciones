                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select	distinct
						e.EditionId,
						e.NumberEdition,
						
						c.ClientId,
						c.ShortName AS CompanyName,
						
						b.BrandId,
						b.BrandName,
						
						cb.AddedDate,
						cbt.ClientBrandTypeId,
						cbt.Description,
null as Count,
null as UserName,
null as Executive,
null as Date,
null as DateOfReport

						
				from ClientBrands cb
				inner join Editions e on cb.EditionId = e.EditionId
				inner join Clients c on cb.ClientId = c.ClientId
				inner join Brands b on cb.BrandId = b.BrandId
left join ClientBrandTypes cbt on cb.ClientBrandTypeId = cbt.ClientBrandTypeId
				WHERE	cb.EditionId = @EditionId AND
						cb.ClientId = @ClientId AND
						cb.BrandId = @BrandId AND
						AddedDate &gt;= CONVERT(varchar(10), @Date, 112) AND
													AddedDate &lt; CONVERT(varchar(10), @Date+1, 112) </CommandText>
