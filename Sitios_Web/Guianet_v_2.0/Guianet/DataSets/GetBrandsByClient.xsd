                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select	DISTINCT
			
					b.BrandId,
					b.BrandName,
					
					cb.ExpireDescription,
					
					CASE WHEN (cb.ClientBrandTypeId = (select ClientBrandTypeId from ClientBrandTypes WHERE Description = 'Distribuidor autorizado'))
					THEN 1 ELSE NULL END AS Distributor,
					
					CASE WHEN (cb.ClientBrandTypeId = (select ClientBrandTypeId from ClientBrandTypes WHERE Description = 'Representante exclusivo'))
					THEN 1 ELSE NULL END AS Representative,

                                                        null as Count,
                                                        null as CompanyName,
                                                        null as Type,
                                                        null as NumberEdition,
null as Adviser
			
			from ClientBrands cb
			inner join Brands b on cb.BrandId = b.BrandId
			where cb.ClientId = @ClientId AND
				  cb.EditionId = @EditionId
			order by b.BrandName</CommandText>
