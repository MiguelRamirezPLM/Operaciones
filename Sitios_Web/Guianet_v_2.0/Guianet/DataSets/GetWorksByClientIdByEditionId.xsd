                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT w.WorkId, e.EditionId, e.NumberEdition, w.ClientId, c.CompanyName, w.ProductId, p.ProductName, w.WorkDescription,w.WorkDescriptionLevelThree , w.AddedDate, w.UserName, w.Module,w.Details,
NULL AS Count,
NULL AS CountP,
NULL AS Adviser,
NULL AS Adviser1,
w.Active
FROM            Works AS w INNER JOIN
                         EditionClients AS ec ON w.ClientId = ec.ClientId AND @EditionId = ec.EditionId AND @ClientTypeId = ec.ClientTypeId INNER JOIN
                         Editions AS e ON ec.EditionId = e.EditionId INNER JOIN
                         Clients AS c ON w.ClientId = c.ClientId INNER JOIN
                         Products AS p ON w.ProductId = p.ProductId
WHERE        (ec.EditionId = @EditionId) AND (ec.ClientTypeId = @ClientTypeId)
ORDER BY c.CompanyName, p.ProductName, w.WorkDescription</CommandText>
