        foreach (Clients _cl in cl)
        {
            <span class="spans">@_cl.CompanyName</span>
        }
                    }
                </td>

            </tr>
        </table>
    }
}
<br />
@{
    if (indexsalesmodule != null)
    {
        <div style="width:100%" class="table-responsive">
            @_datagrid.GetHtml(
                          headerStyle: "webgrid-header",
                          footerStyle: "webgrid-footer",
                          alternatingRowStyle: "webgrid-alternating-row",
                          selectedRowStyle: "webgrid-selected-row",
                          rowStyle: "webgrid-row-style",
                          tableStyle: "webgrid-table1",
                          mode: WebGridPagerModes.All,
                          columns: new[] {
                          _datagrid.Column("", header: "Nombre de Producto", format: @<text>
                                @{
                                    <span class="display-mode"> <label id="lblName">@item.ProductName</label> </span>
                                    <textarea style="display:none; color:red; font-size: 12px;" id="Name" class="edit-mode edprod form-control" onkeypress="removerequieredfieldSM($(this))">@item.ProductName</textarea>
                                    <input id="lblProductid" style="display:none;" value="@item.ProductId" />
                                }
                        </text>, style:"column19"),
         _datagrid.Column("", header: "Registro Sanitario", format: @<text>
                @{
                    <span class="display-mode"> <label id="lblregister">@item.RegisterSanitary</label> </span>
                    <textarea style="display:none; color:red; font-size: 12px;" maxlength="150" id="Register" class="edit-mode edreg form-control">@item.RegisterSanitary</textarea>
                }
        </text>, style:"column19"),
         _datagrid.Column("", header: "Código de Barras", format: @<text>
                @{
                    <span class="display-mode"> <label id="lblbarcode">@item.BarCode</label> </span>
                    <textarea style="display:none; color:red; font-size: 12px;" maxlength="150" id="barcode" class="edit-mode barcode form-control">@item.BarCode</textarea>
                    <input id="lblBarCodeId" style="display:none" value="@item.BarCodeId" />
                }
        </text>, style:"column19"),

            _datagrid.Column("", header: "Reclasificación", format: @<text>
                @{
                    <a target="_blank" href="@Url.Action("GetReassingCategoriesReport", "GetReports", new { id = "PDF", ProductId = @item.ProductId })" class="btn btn-primary btn-sm">
                        <span class="glyphicon glyphicon-edit"></span>
                    </a>
                }
            </text>,style:"text-center column7"),

            _datagrid.Column("", header: "Estado", format: @<text>
                @{
                    <button class="btn btn-primary btn-sm" onclick="getproductstatusSM($(this))" value="@item.ProductId">
                        <span class="glyphicon glyphicon-stats"></span>
                    </button>
                }
            </text>,style:"text-center column7"),

     _datagrid.Column("", header: "Participante", format: @<text>
                @{
                int PP = item.PP;
                                if(PP != 0)
                                {
                                    <input type="checkbox" id="Participant" checked="checked" onclick="checkparticipantproducts($(this))" />
                                }
                                else
                                {
                                    <input type="checkbox" id="Participant" onclick="checkparticipantproducts($(this))" />
                                }
                }
    </text>,style:"text-center column7"),
             _datagrid.Column("", header: "Participante en Impreso", format: @<text>
                @{
                int MP = item.MP;
                                if (MP != 0)
                                {
                                    <input type="checkbox" id="Mentionated" checked="checked" onclick="checkPrintedProducts($(this))" />
                                }
                                else
                                {
                                    <input type="checkbox" id="Mentionated" onclick="checkPrintedProducts($(this))" />
                                }
                }
            </text>,style:"text-center column7"),

    _datagrid.Column("", header: "Con Cambios", format: @<text>
                @{
                                   int CC = item.CC;

                                if(CC != 0)
                                {
                                    <input type="checkbox" id="CC" checked="checked" onclick="insertProductChanges($(this))" />
                                }
                                else
                                {
                                    <input type="checkbox" id="CC" onclick="insertProductChanges($(this))" />
                                }
                }
    </text>,style:"text-center column7"),
    _datagrid.Column("", header: "Sin Cambios", format: @<text>
                @{
                                   int SC = item.SC;

                                if(SC != 0)
                                {
                                    <input type="checkbox" id="SC" checked="checked" onclick="insertProductWithoutChanges($(this))" />
                                }
                                else
                                {
                                    <input type="checkbox" id="SC" onclick="insertProductWithoutChanges($(this))" />
                                }
                }
    </text>,style:"text-center column7"),
        _datagrid.Column("", header: "SIDEF", format: @<text>
                @{
                                   int SIDEF = item.SIDEF;

                                if(SIDEF != 0)
                                {
                                    <input type="checkbox" id="SIDEF" checked="checked" onclick="inserproductSIDEF($(this))" />
                                }
                                else
                                {
                                    <input type="checkbox" id="SIDEF" onclick="inserproductSIDEF($(this))" />
                                }
                }
        </text>,style:"text-center column7"),

                        _datagrid.Column("", header: "Editar", format: @<text>
                @{
                    <button class="edit-user display-mode btn btn-warning btn-sm" onclick="activefieldstoeditproductSM($(this))">
                        <span class="glyphicon glyphicon-edit"></span>
                    </button>
                    <button class="save-user edit-mode btn btn-primary btn-sm" style="display:none" onclick="savechangeseditproductsSM($(this))">
                        <span class="glyphicon glyphicon-ok"></span>
                    </button>
                    <button class="cancel-user edit-mode btn btn-danger btn-sm" style="display:none" onclick="canceleditproductSM($(this))">
                        <span class="glyphicon glyphicon-remove"></span>
                    </button>
                }
                        </text>,style:"text-center column7"),

                        _datagrid.Column("", header: "Clasificar", format: @<text>
                @{
                    <button class="btn btn-danger btn-sm ldr" id="clasifbtn" style="display:normal" onclick="location.href='@Url.Action("ClasificationProducts", "SalesModule", new { ProductId = item.ProductId })';return false;">
                        <span class="glyphicon glyphicon-cog"></span>
                    </button>
                }
                        </text>,style:"text-center column7"),

            _datagrid.Column("", header: "Fabricantes", format: @<text>
                @{
                    <button class="btn btn-primary btn-sm" id="Manufacturersbtn" style="display:normal" onclick="GetManufacturers($(this))">
                        <span class="glyphicon glyphicon-wrench"></span>
                    </button>
                }
            </text>,style:"text-center column7")
        })
        </div>
        <br />
    }
}


@*          AGREGAR PRODUCTO                 *@

<button id="btnaddProduct" data-toggle="modal" data-target="#addProduct" style="display:none"></button>
