                    <CommandText>SELECT        p.ProductName, p.Description, d.DivisionName, pf.PharmaForm, c.CategoryName, 0 AS NumberEdition, NULL AS Type, NULL AS NewP, 0 as Count, NULL as Active, '' as RegisterSanitary
