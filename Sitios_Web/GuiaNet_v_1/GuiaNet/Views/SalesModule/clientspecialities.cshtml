@model IEnumerable<GuiaNet.Models.ClientSpecialities>
@using GuiaNet.Models;
@{
    ViewBag.Title = "clientspecialities";
    Guia db = new Guia();
    WebGrid _datagrid = new WebGrid(Model);
    int ClientIdss, EditionIds, BookIds, CountryIdss;
    sessionclientspecialities ind = (sessionclientspecialities)Session["sessionclientspecialities"];
    if (ind != null)
    {
        ClientIdss = ind.ClId;
        EditionIds = ind.EId;
        BookIds = ind.BId;
        CountryIdss = ind.CId;
    }
    else
    {
        ClientIdss = 0;
        EditionIds = 0;
        BookIds = 0;
        CountryIdss = 0;
    }
}

<html>
<head>
    <title></title>
    @*ADVERS*@
    <script>
        $(document).ready(function () {
            $(".advers").focus(function () {
                var tr = $(this).parents("tr:first");
                var clientId = $("#ClientId").val();
                var edition = $("#EditionId").val();
                var sp = tr.find("#SpecialityId").val();
                tr.find(".advers").blur(function () {
                    var adv = tr.find(".advers").val();
                    var qt = tr.find(".quantity").val();
                    $.ajax({
                        type: "POST",
                        dataType: "json",
                        url: "../SalesModule/insertreferenceadvers/",
                        traditional: true,
                        data: { Client: clientId, Edition: edition, Advers: adv, Speciality: sp, Quantity: qt },
                        success: function (data) {
                            if (data == false) {
                                tr.find(".advers").val('');
                            }
                            if (data == true) {
                                setTimeout('document.location.reload()');
                            }
                        }
                    });
                });

            });
        });
    </script>
    @*END*@
    @*ADVERS SUC*@
    <script>
        $(document).ready(function () {
            $(".adversbranch").focus(function () {
                var tr = $(this).parents("tr:first");
                var clientId = $("#branchid").val();
                var edition = $("#EditionId").val();
                var sp = tr.find("#SpecialityIdsuc").val();
                tr.find(".adversbranch").blur(function () {
                    var adv = tr.find(".adversbranch").val();
                    var qt = tr.find(".quantitybranch").val();
                    $.ajax({
                        type: "POST",
                        dataType: "json",
                        url: "../SalesModule/insertreferenceadvers/",
                        traditional: true,
                        data: { Client: clientId, Edition: edition, Advers: adv, Speciality: sp,Quantity:qt },
                        success: function (data) {
                            if (data == false) {
                                tr.find(".advers").val('');
                            }
                            if (data == true) {
                                setTimeout('document.location.reload()');
                            }
                        }
                    });
                });

            });
        });
    </script>
    @*END*@
    <script>
