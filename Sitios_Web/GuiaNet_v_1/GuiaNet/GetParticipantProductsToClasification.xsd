                    <CommandText>		select	distinct 
		
null as CompanyName,
0 as NumberEdition,
		p.ProductId,
		p.ProductName,
		(select COUNT(*) from ClientProductCategories where ClientId = @ClientId and ProductId = p.ProductId) as Qtty,
		
		(select COUNT(*) from ClientProductLeafCategories where ClientId = @ClientId and ProductId = p.ProductId) as QttyHC, 
