using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace Web.Models
{
    public class SessionDivisionId
    {
        #region Constructor
        public SessionDivisionId(int? DivisionId, string BrandName)
        {
            division = DivisionId;
            Brand = BrandName;
        }

        #endregion

        #region Member

        public int? division;
        public string Brand;

        #endregion

        #region Properties


        public int? DivisionId
        {
            get
            {
                return division;
            }
            set
            {
                division = value;
            }
        }

        public string BrandName
        {
            get
            {
                return Brand;
            }
            set
            {
                Brand = value;
            }
        }
        #endregion
    }
}